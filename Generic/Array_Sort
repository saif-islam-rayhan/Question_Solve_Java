package bd.generic;

import java.util.*;

class Main{
    public static <T>void arraysort(T[] arr){
        Arrays.sort(arr);
    }



    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        Integer[] array= new Integer[n];
        for (int i=0;i<n;i++){
            array[i]=sc.nextInt();
        }
       arraysort(array);
        for(int i=0;i<n;i++){
            System.out.println(array[i]);
        }

    }
    }


